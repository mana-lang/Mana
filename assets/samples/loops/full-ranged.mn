fn Main() {
    mut data a = 0
    mut data b = 0
    data c = 5
    data d = 47

    Print("-- Ranged loop with mutable counter --\n")
    PrintV("Range: {} ", c)
    PrintV("to {}\n\n", d)
    
    loop c..d => mut i {
        PrintV("a: {}\n", a)
        PrintV("b: {}\n", b)
        PrintV("i: {}\n\n", i)

        a += 1
        i += 3
        b = i
    }

    data e = a
    data f = b
}