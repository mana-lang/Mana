fn Main() {
    mut data iterations = 0
    mut data final_index = 0
    
    loop 4..24 => mut i {
        PrintV("iterations: {}\n", iterations)
        PrintV("index tracker: {}\n", final_index)
        PrintV("i: {}\n\n", i)

        iterations += 1
        i += 3
        final_index = i
    }

    PrintV("iterations: {}\n", iterations)
    PrintV("final index: {}\n", final_index)
}