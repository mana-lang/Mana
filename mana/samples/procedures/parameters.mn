fn Main() {
    data sum_of = Summate(Add(SquareIf(3, true), 17))
    PrintV("Summation: {}", sum_of)
}

fn Summate(count: i64) -> i64 {
    mut data a = 0

    loop count => i {
        a += i
    }

    return a
}

fn Add(a, b: i64) -> i64 {
    return a + b
}

fn SquareIf(a: i64, b: bool) -> i64 {
    if b {
        return a * a
    }

    return a
}

